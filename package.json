{
  "name": "indian-pincode-db",
  "version": "1.0.0",
  "description": "Indian Pincode Database",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivekvt/indian-pincode-db.git"
  },
  "homepage": "https://github.com/vivekvt/indian-pincode-db#readme",
  "bugs": {
    "url": "https://github.com/vivekvt/indian-pincode-db/issues"
  },
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "lint": "eslint --ignore-path .eslintignore --fix --ext .js,.ts .",
    "format": "prettier --ignore-path <(cat .gitignore .prettierignore) --write \"**/*.+(js|ts|json)\"",
    "format2": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install",
    "example": "npx ts-node example.ts"
  },
  "author": "vivekvt",
  "license": "MIT",
  "keywords": [
    "indian",
    "pincodes",
    "db",
    "indian-pincode"
  ],
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@types/totp-generator": "^0.0.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.4",
    "rimraf": "^4.4.0",
    "typescript": "^4.9.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/**/*"
  ]
}
